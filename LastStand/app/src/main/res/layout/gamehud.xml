<?xml version="1.0" encoding="utf-8"?>

<LinearLayout
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="fill_parent"
    android:gravity="center_horizontal"
    android:id="@+id/dankEndScreen"
    android:background="#7FB2EBF2"
    xmlns:android="http://schemas.android.com/apk/res/android">

            <TextView
                android:id="@+id/enlesscompleted"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:paddingLeft="0dp"
                android:paddingRight="0dp"


                android:layout_marginTop="50dp"
                android:gravity="top|center_horizontal"
                android:text="Level Completed!"
                android:textSize="18pt"
                android:textColor="#EF5350" />

            <RelativeLayout
                android:gravity="center"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="30dp"
                >
                <ImageView
                    android:layout_width="300dp"
                    android:layout_height="175dp"
                    android:layout_gravity="center"
                    android:layout_margin="5dp"
                    android:text="9"
                    android:width = "20dp"
                    android:height = "20dp"
                    android:gravity="center"
                    android:background="@drawable/endscreenbox"
                    android:textSize="30sp"
                    android:paddingRight="18dp"
                    android:paddingTop="10dp"
                    android:id="@+id/level2"
                    android:layout_column="0" >

                </ImageView>

                <TextView
                    android:id="@+id/coinSlot"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:paddingLeft="0dp"
                    android:paddingRight="0dp"
                    android:layout_alignLeft="@+id/level2"
                    android:layout_alignRight="@+id/level2"

                    android:layout_marginTop="30dp"
                    android:layout_marginRight="20dp"
                    android:gravity="top|right"
                    android:text="Time: 1.224s"
                    android:textSize="12pt"
                    android:textColor="#EF5350" />
                <TextView
                    android:id="@+id/timeSlot"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:paddingLeft="0dp"
                    android:paddingRight="0dp"
                    android:layout_alignLeft="@+id/level2"
                    android:layout_alignRight="@+id/level2"

                    android:layout_marginTop="80dp"
                    android:layout_marginRight="20dp"
                    android:gravity="top|right"
                    android:text="Time: 1.224s"
                    android:textSize="12pt"
                    android:textColor="#EF5350"
                    />
                <TextView
                    android:id="@+id/fastestSlot"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:paddingLeft="0dp"
                    android:paddingRight="0dp"
                    android:layout_alignLeft="@+id/level2"
                    android:layout_alignRight="@+id/level2"

                    android:layout_marginTop="130dp"
                    android:layout_marginRight="20dp"
                    android:gravity="top|right"
                    android:text="Time: 1.224s"
                    android:textSize="12pt"
                    android:textColor="#EF5350" />

                <ImageView
                    android:layout_width="55dp"
                    android:layout_height="55dp"
                    android:layout_gravity="center"
                    android:layout_marginLeft="25dp"
                    android:layout_marginTop="15dp"
                    android:text="9"
                    android:width = "20dp"
                    android:height = "20dp"
                    android:gravity="center"
                    android:background="@drawable/starfull"
                    android:textSize="30sp"
                    android:paddingRight="18dp"
                    android:paddingTop="10dp"
                    android:id="@+id/star1"
                    android:layout_column="0"
                    >

                </ImageView>
                <ImageView
                    android:layout_width="55dp"
                    android:layout_height="55dp"
                    android:layout_gravity="center"
                    android:layout_marginLeft="25dp"
                    android:layout_marginTop="65dp"
                    android:text="9"
                    android:width = "20dp"
                    android:height = "20dp"
                    android:gravity="center"
                    android:background="@drawable/starfull"
                    android:textSize="30sp"
                    android:paddingRight="18dp"
                    android:paddingTop="10dp"
                    android:id="@+id/star2"
                    android:layout_column="0" >

                </ImageView>
                <ImageView
                    android:layout_width="55dp"
                    android:layout_height="55dp"
                    android:layout_gravity="center"
                    android:layout_marginLeft="25dp"
                    android:layout_marginTop="115dp"
                    android:text="9"
                    android:width = "20dp"
                    android:height = "20dp"
                    android:gravity="center"
                    android:background="@drawable/starfull"
                    android:textSize="30sp"
                    android:paddingRight="18dp"
                    android:paddingTop="10dp"
                    android:id="@+id/star3"
                    android:layout_column="0" >

                </ImageView>


            </RelativeLayout>

            <TableLayout
                android:layout_width="fill_parent"
                android:layout_height="fill_parent"
                android:layout_gravity="bottom"
                android:gravity="center">

                <TableRow
                    android:layout_gravity="bottom"
                    android:gravity="center"

                    >
                    <ImageButton
                        android:layout_width="50dp"
                        android:layout_height="50dp"
                        android:id="@+id/home"
                        android:background="@drawable/homebutton"/>
                    <ImageButton
                        android:layout_width="120dp"
                        android:layout_height="120dp"
                        android:layout_margin="20dp"
                        android:id="@+id/restart"
                        android:background="@drawable/restartbutton"
                        />
                    <ImageButton
                        android:layout_width="50dp"
                        android:layout_height="50dp"
                        android:id="@+id/next"
                        android:background="@drawable/arrowbutton"/>
                </TableRow>
                </TableLayout>
